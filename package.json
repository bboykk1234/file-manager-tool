    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^2.2.7",
    "terser-webpack-plugin": "^5.1.4",
    "typescript": "^4.3.5",
    "wait-on": "^6.0.0",
    "webpack": "^5.47.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "cloudinary": "^1.26.2",
